{
  "compilerOptions": {
    "lib": ["esnext"],
    "target": "es6",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@/*": [
        "netlify/*"
      ]
    },
  },
  "include": ["netlify/**/*.ts", "module.d.ts"],
  "exclude": ["node_modules"]
}